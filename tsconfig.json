{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@sections/*": ["src/sections/*"],
      "@icons/*": ["src/icons/*"],
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"],
      "@utils/*": ["src/utils/*"]
    },
    "jsx": "preserve",
    "types": ["astro/client"]
  },
  "exclude": [
    "api-backup/**/*",
    "test-smtp.js",
    "debug-works.js",
    "scripts/**/*",
    "node_modules"
  ]
}
